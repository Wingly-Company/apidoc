{
    "name": "wingly/apidoc",
    "description": "Use php 8 attributes to generate your api documentation",
    "authors": [
        {
            "name": "Dimitris Karapanos",
            "email": "karapanos.dimitris@gmail.com"
        }
    ],
    "homepage": "https://github.com/Wingly-Company/apidoc",
    "require": {
        "php": "^8.0",
        "illuminate/console": "^8.0",
        "illuminate/routing": "^8.0",
        "illuminate/support": "^8.0",
        "michelf/php-markdown": "^1.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "mockery/mockery": "^1.3",
        "orchestra/testbench": "^6.0",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "Wingly\\ApiDoc\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wingly\\ApiDoc\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wingly\\ApiDoc\\ApiDocServiceProvider"
            ]
        }
    },
    "scripts": {
        "format": [
            "vendor/bin/php-cs-fixer fix"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
